<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="KalaCalcu.Map"
             BackgroundImage="mainbg.png">
    <ContentPage.Content>
        <StackLayout>
            <Frame CornerRadius="10"  Margin="40,0,40,0" VerticalOptions="CenterAndExpand" BackgroundColor="#5C9C37" Padding="10,20,10,0" HeightRequest="540">
                <StackLayout>
                    <Label Text="Map" FontSize="28" FontAttributes="Bold"  FontFamily="Montserrat" TextColor="White" HorizontalOptions="Center"></Label>
                    <Image Source="Mapp.png"/>
                </StackLayout>
            </Frame>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>